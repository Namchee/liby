{
  "name": "liby",
  "private": true,
  "version": "1.0.0",
  "description": "Personal quickstart template for TypeScript libraries",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "test": "vitest --passWithNoTests",
    "coverage": "vitest run --coverage",
    "build": "tsup src/index.ts --dts --format esm,cjs --minify",
    "lint": "eslint src/**/*.*",
    "lint:fix": "eslint --fix src/**/*.*",
    "prepare": "husky install"
  },
  "keywords": [
    "boilerplate",
    "eslint",
    "typescript"
  ],
  "author": {
    "name": "Cristopher Namchee",
    "email": "cristophernamchee12@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.7.13",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-config-namchee": "^1.0.13",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.0",
    "rimraf": "^3.0.2",
    "tsup": "^7.1.0",
    "typescript": "5.1.6",
    "typesync": "^0.9.2",
    "vite": "^4.3.9",
    "vitest": "^0.32.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
